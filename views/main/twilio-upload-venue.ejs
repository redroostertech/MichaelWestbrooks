        <script>
            function goBack(){
                window.history.back();
            }
            
            var imageCount = 1;
            var maxCount = 3;
            function appendInput() {
                if (imageCount < maxCount) {
                    console.log('Not Yet');
                    imageCount = imageCount + 1;
                    var input = '<div id="input-item" class="form-name form-element large left" data-provides="fileinput"><div class="fileinput-new thumbnail" style="width: 200px; height: 150px;" data-trigger="fileinput"><img src="http://placehold.it/200x150" alt="..."></div><div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 150px"></div><div><span class="btn btn-white btn-file"><span class="fileinput-new">Select image</span><span class="fileinput-exists">Change</span><input type="file" name="venueImage1" id="field-5-1" accept="image/*"></span><a href="#" class="btn btn-orange fileinput-exists" data-dismiss="fileinput">Remove</a></div></div>';
                    console.log(imageCount);
                    $('#floors').val(imageCount);
                    $('#uploadCount').append(input);
                } else {
                    console.log('Ok');
                    alert('Error: You can only add up to 3 floorplans.');
                }
            }
            
            function removeInput() {
                imageCount = imageCount - 1;
                console.log(imageCount);
                $('#input-item').last().remove();
            }
