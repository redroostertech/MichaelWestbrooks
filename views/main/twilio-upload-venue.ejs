                                            <div class="column width-12">
                                                <input type="text" id="venuestate"  name="venuestate" class="form-name form-element large left" placeholder="Venue State">
                                            </div>
                                            <div class="column width-12">
                                                <input type="number" id="sectioncount" name="sectioncount" class="form-name form-element large left" placeholder="Number of Sections" required>
                                            </div>
                                            <div class="column width-12">
                                                <div class="column width-12" id="uploadCount">
                                                    <div class="form-name form-element large left" data-provides="fileinput">
                                                        <div class="fileinput-new thumbnail" style="width: 200px; height: 150px;" data-trigger="fileinput">
                                                            <img src="http://placehold.it/200x150" alt="...">
                                                        </div>
                                                        <div class="fileinput-preview fileinput-exists thumbnail" style="max-width: 200px; max-height: 150px">
                                                        </div>
                                                        <div>
                                                            <span class="btn btn-white btn-file">
                                                                <span class="fileinput-new">Select image</span>
                                                                <span class="fileinput-exists">Change</span>
                                                                <input type="file" name="venueImage1" id="field-5-1" accept="image/*">
                                                            </span>
                                                            <a href="#" class="btn btn-orange fileinput-exists" data-dismiss="fileinput">Remove</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="column width-12">
                                                <a href="#" onclick="appendInput()" class="button medium text-uppercase bkg-theme bkg-hover-charcoal color-white color-hover-white">
                                                    <i class="entypo-plus"></i>
                                                    Add Floorplan Level
                                                </a>
                                                <a href="#" onclick="removeInput()" class="button medium text-uppercase bkg-theme bkg-hover-charcoal color-white color-hover-white">
                                                    <i class="entypo-cancel"></i>
                                                    Remove Last Floorplan Level
                                                </a>
                                            </div>
                                            <div class="column width-12" style="display: none;" >
                                                <input type="number" id="floors" name="floors" class="form-name form-element large left" placeholder="Number of Floors" value="1" required>
                                            </div>
                                            <div class="column width-12" >
